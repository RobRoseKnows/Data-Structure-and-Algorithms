#include<stdlib.h>
#include "link_list.h"

struct LNode* create_link_list(int array[], int n){

	struct LNode* head = (struct LNode*)malloc(sizeof(struct LNode));
	struct LNode* p = head;
	int i = 0;
	for(i=0; i<n; i++){
		p->data = array[i];
		p->next = (struct LNode*)malloc(sizeof(struct LNode));
		p = p->next;
	}

	p->next = head;
	return head;
}
