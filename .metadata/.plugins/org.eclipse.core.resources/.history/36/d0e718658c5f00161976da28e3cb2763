#define NUM 100
#include<stdio.h>
#include"several_algs.h"
int main(int argc, char* argv[]){
	int ori_array[] = {23,24,22,1,-2,5,6,9,10,-3,15};
	int n = 11;
	int ret_ins = insert_sort(ori_array, n);
	print("insert_sort", ori_array, n);
	int ret_bub = bubble_sort(ori_array, n);
	print("bubble_sort", ori_array, n);
	int ret_sel = selection_sort(ori_array, n);
	print("selection_sort", ori_array, n);
	int ret_qui = quick_sort(ori_array, 0, n-1);
	print("quick_sort", ori_array, n);
	return OK;
}
