#include<stdio.h>
#include"several_algs.h"

int insert_sort(int ori_array, int n){
	int i = 0;
	int j = 0;
	for(i=1; i<n; i++){
		for(j=i; j>0; j--){//start from i --> 0
			if(ori_array[j] < ori_array[j-1]) swap(&ori_array[j], &ori_array[j-1]);
			else break;
		}
	}

	for(i=0; i<n; i++)
		printf("%d ", ori_array[i]);
	printf("\n");
	return OK;
}
