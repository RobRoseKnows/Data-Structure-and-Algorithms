#ifndef QUEUE_H_
#define QUEUE_H_

#define INIT_CAPACITY 10
struct Queue{
	int *data;
	int queue_size;
	int head;
	int rail;
};
void init_queue(struct Queue* queue);
void enqueue(struct Queue* queue, int e);
int dequeue(struct Queue* queue);
int queue_peek(struct Queue* queue);
#endif /* QUEUE_H_ */
