#include <stdio.h>
#include "link_list.h"
int main(int argc, char* argv[]){
	int array[] = {2, 3, 1};
	int n = 9;
	struct LNode* head = create_link_list(array, n);
	struct LNode* p = head;
	printf("create list?\n");
	while(p->next != head){
		printf("%d ", p->data);
		p = p->next;
	}
}
