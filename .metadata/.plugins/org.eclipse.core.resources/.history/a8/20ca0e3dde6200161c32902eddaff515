#include<stdlib.h>
#include<stdio.h>

struct BSTNode* create_bstree(int array[], int n){
	if(n==0) return NULL;
	struct BSTNode* root = (struct BSTNode*)malloc(sizeof(struct BSTNode));
	root->data = array[0];

	struct BSTNode* p = root;
	int i = 0;
	for(i=0; i<n; i++){
		struct BSTNode* tmp = (struct BSTNode*)malloc(struct BSTNode);

	}
}
