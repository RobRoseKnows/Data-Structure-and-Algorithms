/**
 * quick sort
 * unstable
 * time: worst, O(n^2), average, O(nlogn); best, O(nlogn)
 * auxiliary:O(nlogn)?why?
 */
#include"several_algs.h"

int quick_sort(int ori_array[], int l, int r){
	if(l < r){
		int pos = partition(ori_array, l, r);

		int ret1 = quick_sort(ori_array, l, pos-1);
		int ret2 = quick_sort(ori_array, pos+1, r);
	}
}
